    <script>
        $(window).load(function () {
            //  alert("window load occurred!");
            var color = window.localStorage.getItem('color-key');
            // alert(color);
            $('body').addClass(color);
        });
        $(document).ready(function () {
            $(".spinner").click(function () {
                $(".color-picker").toggleClass("color-spinner");
            });

            $('.red').click(function () {
                $('body').removeClass().addClass('red');
                localStorage.setItem('color-key', 'red');
            });
            $('.blue').click(function () {
                $('body').removeClass().addClass('blue');
                localStorage.setItem('color-key', 'blue');
            });
            $('.black').click(function () {
                $('body').removeClass().addClass('black');
                localStorage.setItem('color-key', 'black');
            });
            $('.orange').click(function () {
                $('body').removeClass().addClass('orange');
                localStorage.setItem('color-key', 'orange');
            });
            $('.purple').click(function () {
                $('body').removeClass().addClass('purple');
                localStorage.setItem('color-key', 'purple');
            });
            $('.royal-blue').click(function () {
                $('body').removeClass().addClass('royal-blue');
                localStorage.setItem('color-key', 'royal-blue');
            });
            $('.default-color').click(function () {
                $('body').removeClass();
                localStorage.removeItem('color-key', 'royal-blue');
            });

        });

    </script>
